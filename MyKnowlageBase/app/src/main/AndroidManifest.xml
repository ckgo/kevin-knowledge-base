<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="base.knowlage.com.myknowlagebase">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.CKMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.dialog.CKDialogActivity" />
        <activity android:name=".activity.listview.CKMainListViewActivity" />
        <activity android:name=".activity.listview.CKListViewActivity" />
        <activity android:name=".activity.listview.CKRecyclerviewActivity" />
        <activity android:name=".activity.dialog.CKPopWinActivity"></activity>
    </application>

</manifest>